create table t1 (
pk int not null, 
a varchar(64),
b varchar(64),
c varchar(64)
) engine=innodb;
insert into t1 select
seq, seq, seq, seq
from
seq_1_to_10000;
# Note the r_engine_stats below.  Only non-zero members are printed
ANALYZE FORMAT=json_ext 
select * from t1 where pk < 120000;
ANALYZE
{
  "query_optimization": {
    "r_total_time_ms": "REPLACED"
  },
