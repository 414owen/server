    "r_loops": 1,
    "r_total_time_ms": "REPLACED",
    "nested_loop": [
      {
        "table": {
          "table_name": "t1",
          "access_type": "ALL",
          "r_loops": 1,
          "rows": 10000,
          "r_rows": 10000,
          "r_table_time_ms": "REPLACED",
          "r_other_time_ms": "REPLACED",
          "r_engine_stats": {
            "pages_accessed": 31
          },
          "filtered": 100,
          "r_filtered": 100,
          "attached_condition": "t1.pk < 120000"
        }
